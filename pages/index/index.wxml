<!--index.wxml-->
<view class="container">
  <!-- <search></search> -->
  <view class="index-top">
    

    <view class="index-swiper">
      <swiper indicator-dots autoplay circular interval="5000" duration="500" previous-margin="0px" next-margin="0px">
        <block wx:for="{{background}}" wx:key="index">
          <swiper-item>
            <image src="{{item}}"></image>
          </swiper-item>
        </block>
      </swiper>
    </view>

  </view>

  <view class="index-topbar">
      <view data-src="../need/need" bindtap="gotoUrl">
        <image src="../../images/sec-indeximg_07.png"></image>
        <text>找需求</text>
      </view>
      <view data-src="../expert/expert" bindtap="gotoUrl">
        <image src="../../images/sec-indeximg_09.png"></image>
        <text>找专家</text>
      </view>
      <view data-src="../gainDetail/gainDetail" bindtap="gotoUrl">
        <image src="../../images/sec-indeximg_11.png"></image>
        <text>找成果</text>
      </view>
      <view data-src="../serviceDetail/serviceDetail" bindtap="gotoUrl">
        <image src="../../images/sec-indeximg_13.png"></image>
        <text>找服务</text>
      </view>
      <view data-src="../serviceDetail/serviceDetail" bindtap="gotoUrl">
        <image src="../../images/sec-indeximg_15.png"></image>
        <text>找团队</text>
      </view>
    </view>

  <view class="index-topbar1">
      <view data-src="../need/need" bindtap="gotoUrl">
        
        <text>技术共享</text>
      </view>
      <view data-src="../expert/expert" bindtap="gotoUrl" style="background-image:url(data:image/png;base64,)">
        
        <text>服务转化</text>
      </view>
      <view data-src="../gainDetail/gainDetail" bindtap="gotoUrl" style="background-image:url(data:image/png;base64,)">
        
        <text>免费对接</text>
      </view>
    </view>

  <view class="index-title" style="border-top:10px solid #f1f2f5; margin-top:10px;">
    <text>热门需求</text>
    <button size="mini" plain data-src="../need/need" bindtap="gotoUrl">更多</button>
  </view>
  <view class="index-need">
    <view wx:for="{{needData}}" wx:key="index">
      <b>待合作</b>
      <text class="need-title">{{item.title}}</text>
      <view class="need-center">
        <view>
          <h5>{{item.price}}
            <span>万</span>
          </h5>
          <h6>计划投入</h6>
        </view>
        <view>
          <h5>{{item.year}}
            <span>年</span>
          </h5>
          <h6>解决期限</h6>
        </view>
      </view>
      <view class="need-footer">
        <text wx:for="{{item.label}}" wx:key="index">
          {{item}}
        </text>
      </view>
    </view>
  </view>

  <view class="banner2">
    <image src="../../images/sec-indeximg_32.png" mode="widthFix"></image>
  </view>

  <view class="index-title" style="border-top:10px solid #f5f5f5;">
    <text>技术成果</text>
    <button size="mini" plain data-src="../gainDetail/gainDetail" bindtap="gotoUrl">更多</button>
  </view>
  <view class="cg" style="padding-bottom:50px;">
    <view wx:for="{{needData}}" wx:key="index">
      <image src="../../images/sec-indeximg_35.png" mode="widthFix"></image>
      <text class="need-title" style="padding:5px 15px; padding-left:0; padding-top:0; text-align:left; margin-left:80px; border-bottom: 1px solid #f0f1f4;">{{item.title}}</text>
      <span>行业：机械/机械输送</span>
      <view class="need-footer" style="padding:0 15px; padding-left:80px; margin-top:-5px;">
        <text wx:for="{{item.label}}" wx:key="index">
          {{item}}
        </text>
      </view>
    </view>
  </view>

  <view class="index-fixed">
    <text>热线电话：12351</text>
    <text style="float:right;"> 服务时间：工作日：8：30-17:00</text>
  </view>



</view>