/**index.wxss**/
@import "../../libs/weui.wxss";

.index-top {
  
  background: url(data:image/png;base64,) no-repeat center top;
  background-size: contain;
  padding:10px 15px;
  padding-bottom: 0px;
}

.index-topbar {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  text-align: center;
  background-color: #fff;
  border-radius: 5px;
  box-shadow: 0px 5px 5px rgba(0,0,0, .1)
}
.index-topbar view{
  text-align: center;

}
.index-topbar image{
  width: 35px;
  height:35px;
  margin-left: 5px;
}
.index-topbar text{
  display: block;
  font-size: 12px;
  color: #999;
}
.index-topbar1 {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  text-align: center;
  background-color: #fff;
  
}
.index-topbar1 view{
  text-align: left;
  background: url(data:image/png;base64,) no-repeat right bottom;
  background-size: 80px;
  background-color: #f8f8f8;
  width:30%;
  height:70px;
  box-sizing: border-box;
  padding: 5px 10px;
  border-radius: 5px;

}
.index-topbar1 image{
  width: 35px;
  height:35px;
  margin-left: 5px;
}
.index-topbar1 text{
  display: block;
  font-size: 14px;
  color: #333;
  font-weight: bold;
}
.index-swiper{

  margin: 15px 0 0;
  margin-top: 20px;
  border-radius: 5px;
  overflow: hidden;
  padding-bottom: 10px;
}

.index-swiper swiper-item{
  border-radius: 5px;
}
.index-swiper swiper-item image{
  width: 100%;
  height: 100%;
}
.index-title{
  padding: 6px 15px;
  
  border-bottom: 2px solid #eee;
  position: relative;
  
}
.index-title::after{
  content: '';
  display: block;
  height: 2px;
  width: 60px;
  position: absolute;
  left: 15px;
  bottom: -2px;
  background-color: #3878f6;
}
.index-title text{
   
  font-size: 14px;
  color: #333;
  font-weight: 600;
}
.index-title button{
  float: right;
  
  border: none 0;
  color: #999;
  margin-right: -15px;
}
.index-need{
  overflow: hidden;
  padding: 10px;
}
.index-need>view{
  float: left;
  width: calc(50% - 10px);
  box-sizing: border-box;
  margin: 5px;
  box-shadow: 0px 0px 10px rgba(0,0,0, .1);
  padding: 10px;
  padding-top: 25px;
  border-radius: 4px;
  height: 150px;
  overflow: hidden;
  position: relative;
}
.index-need>view b{
  position: absolute;
  right: 0;
  top: 0;
  display: block;
  width: 50px;
  height: 20px;
  background-color: #ff484a;
  color: #fff;
  box-sizing: border-box;
  padding-left: 5px;
  font-size: 12px;
  border-bottom-left-radius: 5px;
}
.need-title{
  font-size: 14px;
  text-align: center;
  display: block;
  overflow: hidden;    
  text-overflow: ellipsis; 
  white-space: nowrap; 
  color: #333;

}
.need-center{
  overflow: hidden;
  padding-top: 10px;
  height: 50px;
}
.need-center>view{
  float: left;
  width: 50%;
  box-sizing: border-box;
  color: #333;
}
.need-center>view:first-child{
  border-right: 1px solid #eee;
}
.need-center>view h5{
  display: block;
  font-size: 18px;
  text-align: center;
  font-weight: bold;
}
.need-center>view h5 span{
  font-size: 14px;
  font-weight: normal;

}
.need-center>view h6{
  display: block;
  font-size: 12px;
  text-align: center;
  color: #999999;
}
.need-footer{
  padding-top: 3px;
}
.need-footer>text{
  font-size: 11px;
  line-height: 0;
  height: 0;
  display: inline-block;
  background-color: #eff4fe;
  padding: 10px 3px;
  margin-right: 5px;
  margin-bottom: 15px;
  color: #3878f6;
}
.index-need image{
  width: 100%;
  
}
.index-fixed{
  width: 100%;
  position: fixed;
  bottom: 0;
  line-height: 30px;
  font-size: 12px;
  color: #3669f8;
  box-sizing: border-box;
  padding:0 15px;
  background-color: #f1f5fe;
  z-index: 10;
}

.banner2 image{
  width: 100%;
  display: block;
}
.cg image{
  width: 70px;
  height: 70px;
  float: left;
}
.cg{
  padding: 15px;
  
}
.cg>view{

  box-sizing: border-box;
  
  box-shadow: 0px 0px 10px rgba(0,0,0, .1);
  padding: 10px;
  height: 90px;
  border-radius: 5px;
  margin-bottom: 10px;
  overflow: hidden;
  position: relative;
}
.cg>view span{
  font-size: 12px;
  color: #999;
  display: block;
  padding-left: 80px;
  padding-top: 3px;
}